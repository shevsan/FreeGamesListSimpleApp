<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav"
    app:startDestination="@id/mainFragment">

    <fragment
        android:id="@+id/mainFragment"
        android:name="ua.oshevchuk.testrecyclerretrofit.screens.main.MainFragment"
        android:label="fragment_main"
        tools:layout="@layout/fragment_main" >
        <action
            android:id="@+id/action_mainFragment_to_propertiesFragment"
            app:destination="@id/propertiesFragment" />
    </fragment>
    <fragment
        android:id="@+id/propertiesFragment"
        android:name="ua.oshevchuk.testrecyclerretrofit.screens.properties.PropertiesFragment"
        android:label="fragment_properties"
        tools:layout="@layout/fragment_properties" />
</navigation>